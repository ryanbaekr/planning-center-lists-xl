{
    "manifest_version": 3,
    "name": "Planning Center Lists XL",
    "description": "Increase photo size and resolution for a generated list.",
    "version": "1.0.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["https://people.planningcenteronline.com/reports/*"]
        }
    ]
}
